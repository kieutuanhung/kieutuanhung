#include<stdio.h>
#include<math.h>
int main(){
	float diem;
	printf("nhap diem: ");
	while(1){
		scanf("%f",&diem);
		if(diem<=10&&diem>=0){
			break;
		}
		printf("nhap lai diem: ");
	}
	if(diem<5) printf("yeu");
	else if(diem>=5&&diem<7) printf("tb");
	else if(diem>=7&&diem<8) printf("kha");
	else if(diem>=8&&diem<9) printf("gioi");
	else printf("xs");
}
