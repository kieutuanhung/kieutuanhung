#include<stdio.h>
#include<math.h>
#define ll long long
//rivershen
int main(){
	printf("nhap so pt mang: ");
	int n;
	scanf("%d",&n);
	int a[n];
	printf("nhap cac pt mang: ");
	for(int i=0;i<n;i++){
		scanf("%d",&a[i]);
	}
	printf("nhap x: ");
	int x;
	scanf("%d",&x);
	int dem=0;
	for(int i=0;i<n;i++){
		if(a[i]==x) dem++;
	}
	printf("so lan x co la: %d",dem);
}
